{
  "kind": "collectionType",
  "collectionName": "categorias",
  "info": {
    "singularName": "categoria",
    "pluralName": "categorias",
    "displayName": "categoria"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "nombre",
      "required": true
    },
    "descripcion": {
      "type": "text"
    },
    "destacado": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "imagen": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "categorias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::categoria.categoria",
      "mappedBy": "categoria"
    },
    "categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria",
      "inversedBy": "categorias"
    },
    "productos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::producto.producto",
      "mappedBy": "categoria"
    }
  }
}
